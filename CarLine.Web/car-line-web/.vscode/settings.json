{
  "eslint.nodePath": "./node_modules",
  "eslint.packageManager": "npm",
  "eslint.options": {
    "configFile": "./eslint.config.js"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": false
}
